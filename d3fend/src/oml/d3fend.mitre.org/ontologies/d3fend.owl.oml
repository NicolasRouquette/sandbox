	@rdfs:label "Web Authentication"
	aspect WebAuthentication < Authentication [
		restricts some may-create to SessionCookie
	]

	@definition "A web file resource is a file resource identified by a Uniform Resource Identifier (URI) and made available from one host to another host via a web protocol and across a network or networks."
	@rdfs:label "Web File Resource"
	aspect WebFileResource < NetworkFileResource, WebResource [
		restricts some addressed-by to URL
	]

	@definition "Web network traffic is network traffic that uses a standard web protocol."
	@rdfs:label "Web Network Traffic"
	aspect WebNetworkTraffic < NetworkTraffic

