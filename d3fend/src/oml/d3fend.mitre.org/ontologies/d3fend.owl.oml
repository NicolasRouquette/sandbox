	@rdfs:label "Decoy Artifact"
	@rdfs:seeAlso "http://dbpedia.org/resource/Deception_technology"
	@rdfs:seeAlso "https://doi.org/10.1007/978-3-319-25133-2"
	@rdfs:seeAlso "https://shield.mitre.org/"
